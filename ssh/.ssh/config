### Hosts outside my control

Host aur.archlinux.org
    IdentityFile ~/.ssh/aur
    User aur

### Hosts under my personal control

Host home_pi
    HostName drng.ddns.net
    Port 25018
    IdentityFile ~/.ssh/personal
    LocalForward 8888 127.0.0.1:631
    User alarm

Host tunnel_to_home_pi
    HostName drng.ddns.net
    Port 443
    IdentityFile ~/.ssh/personal
    User alarm
    ProxyCommand corkscrew 127.0.0.1 3128 %h %p

Host home_laptop
    HostName drng.ddns.net
    Port 16484
    IdentityFile ~/.ssh/personal
    User ngoonee

Host home_router
    HostName bethsimchatadonai.noip.me
    Port 25008
    IdentityFile ~/.ssh/personal
    User admin
    LocalForward 25008 192.168.0.1:80

Host usj2_router
    HostName ngfamily.hopto.org
    Port 25060
    IdentityFile ~/.ssh/personal
    User admin
    LocalForward 25060 192.168.1.1:80

Host office_pi
    Hostname 192.168.21.101
    Port 65302
    IdentityFile ~/.ssh/personal
    User alarm

Host tunnel_to_office_pi
    Hostname 127.0.0.1
    Port 22222
    IdentityFile ~/.ssh/personal
    User alarm
    ProxyCommand ssh -q home_pi nc %h %p

