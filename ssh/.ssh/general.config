#Host *
#    ControlMaster auto
#    ControlPath ~/.ssh/master-%r@%h:%p
AddKeysToAgent yes

### Hosts outside my control

Host aur.archlinux.org
    IdentityFile ~/.ssh/aur
    User aur

### Hosts under my personal control

Host home_laptop
    HostName bethsimchatadonai.duckdns.org
    Port 16484
    IdentityFile ~/.ssh/personal
    User ngoonee
    ForwardX11 yes
    ForwardX11Trusted yes
    LocalForward 25008 192.168.0.1:80
    LocalForward 30743 127.0.0.1:30743

Host usj2_backup
    HostName ngfamily.duckdns.org
    Port 25060
    IdentityFile ~/.ssh/personal
    User ngfamily

Host office_pi
    HostName 192.168.37.204
    Port 65302
    IdentityFile ~/.ssh/personal
    User alarm
    ProxyCommand ssh -q research_server nc %h %p

