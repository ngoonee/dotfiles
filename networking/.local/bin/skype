if [ -s /home/conf/.current_proxy ]; then
  export http_proxy=`cat /home/conf/.current_proxy`
  export https_proxy=`cat /home/conf/.current_proxy`
  export ftp_proxy=`cat /home/conf/.current_proxy`
  export no_proxy="localhost,127.0.0.0.1,localaddress,.localdomain.com"
fi
export XLIB_SKIP_ARGB_VISUALS=1 && /usr/bin/skype

exit 0
