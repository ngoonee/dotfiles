#!/bin/bash
#rc.d stop ntpd || true;
systemctl stop ntpd.service || true;
systemctl stop cntlm.service || true;
echo -n > /home/conf/my_ip || true;
echo -n > /home/conf/.current_proxy || true;
su ngoonee -c "/home/apps/CyberoamLinuxClient/crclient -l noe" || true;
rm /tmp/CyberClient.noe || true;
ln -sf /home/conf/proxy.pac.direct /home/conf/proxy.pac
killall offlineimap || true;
killall tinyproxy || true;
exit 0
