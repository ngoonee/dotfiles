#!/bin/sh
docker run -d --name bitwarden \
    -e ROCKET_TLS='{certs="/ssl/live/bethsimchatadonai.noip.me/fullchain.pem",key="/ssl/live/bethsimchatadonai.noip.me/privkey.pem"}' \
