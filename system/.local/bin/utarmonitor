#!/bin/sh
# cvt 1280 1024 to get the modeline
xrandr --newmode "1366x768_59.8" 85.00 1368 1440 1576 1784 768 771 781 798 -hsync +vsync
#xrandr --newmode "1280x1024_59.99"  109.00  1280 1368 1496 1712  1024 1027 1034 1063 -hsync +vsync
xrandr --addmode VGA1 1366x768_59.8
#xrandr --addmode VGA1 1280x1024_59.99
xrandr --output LVDS1 --auto --output VGA1 --mode 1366x768_59.8 --rotate normal --left-of LVDS1
#xrandr --output LVDS1 --mode 1280x1024 --output VGA1 --mode 1280x1024_59.99 --same-as LVDS1
